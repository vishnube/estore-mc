{
  "name": "pdfmake",
  "version": "0.2.0-alpha.0",
  "description": "Client/server side PDF printing in pure JavaScript",
  "main": "js/index.js",
  "esnext": "src/index.js",
  "browser": "build/pdfmake.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "iconv-lite": "^0.6.0",
    "linebreak": "^1.0.2",
    "pdfkit": "^0.11.0",
    "svg-to-pdfkit": "^0.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "babel-loader": "^8.1.0",
    "brfs": "^2.0.2",
    "core-js": "^3.6.5",
    "eslint": "^7.3.1",
    "eslint-plugin-jsdoc": "^28.5.1",
    "expose-loader": "^1.0.0",
    "fancy-log": "^1.3.3",
    "file-saver": "^2.0.2",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "rewire": "^5.0.0",
    "sinon": "^9.0.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "transform-loader": "^0.2.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "run-s build build:fonts mocha lint",
    "build": "run-s build:node build:browser",
    "build:node": "babel src --out-dir js",
    "build:browser": "webpack",
    "build:browser-standardfonts": "webpack --config webpack-standardfonts.config.js",
    "build:fonts": "node build-fonts.js",
    "build:examples": "node build-examples.js",
    "lint": "eslint \"./src/**/*.js\" \"./tests/**/*.js\"",
    "mocha": "mocha --reporter spec \"./tests/**/*.spec.js\"",
    "playground": "node dev-playground/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bpampuch/pdfmake.git"
  },
  "keywords": [
    "pdf",
    "javascript",
    "printing",
    "layout"
  ],
  "author": "Bartek Pampuch <bartosz.pampuch@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bpampuch/pdfmake/issues"
  },
  "homepage": "http://pdfmake.org",
  "config": {
    "blanket": {
      "pattern": "src",
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  }
}
